package com.klstudio.net.http {

	/**
	 * HttpCodecUtil
	 * @author Kinglong(kinglong@gmail.com)
	 * @since:2010-8-6
	 */	

	public class HttpCodecUtil {
		//space ' '
		public static const SP : String = ' ';

		
		/**
		 * Carriage return
		 */
		public static const CR : String = '\r';

		/**
		 * Equals '='
		 */
		public static const EQUALS : String = '=';

		/**
		 * Line feed character
		 */
		public static const LF : String = '\n';

		/**
		 * carriage return line feed
		 */
		public static const CRLF : String = '\r\n';

		/**
		 * Colon ':'
		 */
		public static const COLON : String = ':';

		/**
		 * Semicolon ';'
		 */
		public static const SEMICOLON : String = ';';

		/**
		 * comma ','
		 */
		public static const COMMA : String = ',';

		public static const DOUBLE_QUOTE : String = '"';

		public static const DEFAULT_CHARSET : String = "UTF-8";
	}
}
